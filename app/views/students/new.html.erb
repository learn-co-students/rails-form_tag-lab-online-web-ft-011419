<p>Student Form</p>
<%= form_tag students_path do %>
  <p>First Name</p>
  <%= text_field_tag :first_name %>
  <p>Last Name</p>
  <%= text_field_tag :last_name %>
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <%= submit_tag "Submit New Student" %>
<% end %>

<%= session[:formdata] %>
